function repo_pages_init() {
    
    drupal_register_shutdown_function(
        function () {
            
            if ( !empty($GLOBALS['drupal_output_buffering_start_time']) ) {

                $status = drupal_get_http_header("status");
                if ( $status == "404 Not Found" ) {
            
                    $uriWithoutQueryString = strtok($_SERVER['REQUEST_URI'], '?');
                    $testTarget = "sites/all/pages/{$uriWithoutQueryString}.page.php";
                    $testTarget2 = "sites/all/pages/{$uriWithoutQueryString}/.page.php";
                    if ( file_exists($testTarget) || file_exists($testTarget2) ) {
                    
                        @ob_end_clean();
                        while (@ob_end_clean());
                        print "
                            <h1>Inconsistency in Drupal menu-cache detected!</h1>
                            For some reason Drupal did not pick up the fact that the file {$testTarget} exists...<br/> 
                            This can happen when you don't flush Drupal menu-cache after creating a new file in sites/all/pages, doing an SVN update, 
                            or if the menu-cache is invalidated or corrupted.<br/>
                            <br/>
                            <div style=\"border: 1px solid gray; padding: 15px; font-weight: bold;\">
                                <img style=\"width: 25px; padding-right: 15px;\" src=\"data:image/gif;base64,\" />
                                Please wait, flushing Drupal cache... 
                            </div>
                            <br/>
                            Once this [cache-flush] process is complete, this page will automatically reload. If this process takes more then 5 minutes, please manually refresh this page.<br/>
                            <br/>
                            <i>This message has been triggered and rendered from repo_pages.module Coder Bookmark: CB-U318HUV-BC/</i>
                        ";
                        
                        error_log('Clearing all Drupal-Menu caches as per repo_pages.module Coder Bookmark: CB-PKHMG6N-BC');
                        variable_set('menu_rebuild_needed', true);
                        menu_rebuild();
                        
                        print '<script>document.location.reload();</script>';
                    }
                }
            } else {
                error_log(
                    'WARNING: You do not appear to have the drupal_output_buffering module enabeled.' . 
                    'Without this the repo_pages module cannot notify the use when there is a false-positive on a 404 error (due to Drupal-menu-caching issues)'
                );
            }
        }
    );
    
}

/**
 * Implements hook_menu().
 */
function repo_pages_menu() {
    
    $items = array();
    
    $items['repo_pages_module/js-css-read'] = array(
        'title' => 'Hidden JavaScript/CSS Access Workaround (RepoPages Module)',
        'description' => 'JavaScript/CSS filenames that has a dot/period as the first character may not be accessible througn Apache, this menu hook acts as a workaround for this.',
        'page callback' => 'repo_pages_javascript_serv_workaround',
        'access callback' => true,
        'type' => MENU_CALLBACK,
    );
    
    $containerDirectory = 'sites/all/pages/';
    $repoPages = _repo_pages_find_pages_files($containerDirectory);
    foreach ( $repoPages as $urlPath => $pageInfo ) {

        $urlPath = str_replace($containerDirectory, '', $urlPath);
        $pageInfo['title'] = strval($pageInfo['title']);
        error_log("repo_pages module is registering the Drupal-menu path of $urlPath with the title of \"{$pageInfo['title']}\" to execute the script at {$pageInfo['path']} ");
        $items[$urlPath] = array(
            'title' => ( $pageInfo['title'] === '' ? '<none>' : $pageInfo['title'] ),
            'description' => 'Invokes file: ' . $pageInfo['path'],
            'page callback' => 'repo_pages_fetch_page',
            'page arguments' => array(
                $pageInfo['path'],
                module_exists('drupal_output_buffering')
            ),
            'access callback' => true,
            'type' => MENU_CALLBACK,
        );
    
    }
    
    error_log("repo_pages module has finished registering Drupal menu-paths. ");
    
    return $items;
}

/** mixed repo_pages_fetch_page(string $filePath)
  *
  * If the $filePath does not exist, RETURNS FALSE
  * If the $filePath exists, the $filePath is run through include() ...
  *   if the include() function returns anything other than the integer 1, the returned value from include() is returned by this function.
  *   if the include() function returns the integer 1 (which is default for this function), all print statements called from executing this $filePath-file is returned (HTML is returned)
  */
function repo_pages_fetch_page($filePath = '', $allowSoloMode = false) {
    
    if ( !file_exists($filePath) ) {
        return false;
    }
    
    $pageInfo = _repo_pages_get_page_info($filePath);

    // If this is a full-path, translate this to a relative path
    if ( substr($filePath, 0, 1) === '/' || substr($filePath, 1, 2) === ":\\" ) {
        $filePath = realpath($filePath);
        $filePath = str_replace(realpath(DRUPAL_ROOT), '', $filePath);
        $filePath = str_replace("\\", '/', $filePath);
        $filePath = ltrim($filePath, '/');
        @header("RepoPagePath: $filePath");
    }
    
    @header( "RepoPageFinalPath: " . $filePath );
    
    $html = '';

    // Debug and bug killers
    if ( $pageInfo['render mode'] != 'solo' || intval($allowSoloMode) !== 1 ) {
    
        $printr = print_r($pageInfo, true);
        $html .= "
            <!--
                DEBUG INFO: repo_pages_fetch_page
                    allowSoloMode = {$allowSoloMode}
                    _repo_pages_get_page_info( filePath ) = {$printr}
            -->
        ";
        
        $html .= "
            <!-- Bug Killer - The title of pages should be pulled from the ~.page.info file. /* Coder Bookmark: CB-YO4XWCR-BC */ -->
            <script rendersource=\"" . basename(__FILE__) . "\">
                jQuery(document).ready( function () {
                    /* Bug Killer - The title of pages should be pulled from the ~.page.info file. Coder Bookmark: CB-2C7SSZB-BC */
                    jQuery('#titleWrapper h1').text('{$pageInfo['title']}');
                    jQuery('#page-title').text('{$pageInfo['title']}');
                    document.title = '{$pageInfo['title']}';
                    jQuery('#titleWrapper h1').attr('note', 'altered by jQuery at Coder Bookmark: CB-2C7SSZB-BC');
                });
            </script>
        ";
    }
    
    // Autodetect and include TARGET.page.inc file
    $incPath = str_replace('.page.php', '.page.inc', $filePath);
    $incPath = $incPath;
    if ( file_exists($incPath) ) {
    
        repo_pages_codescan_scaninc($pageInfo, $incPath);
        ob_start();
        include($incPath);
        
        // We do not allow print statements in this .inc convention
        if ( variable_get('repo_pages_scan', true) === true && $pageInfo['code scan'] == 'strict' ) {
            if ( trim(ob_get_contents()) !== '' ) {
                repo_pages_codescan_reportissue(0, $incPath, '*.page.inc files are not allowed to print or contain markup. Please remove any print statements from this file. If this file needs to return an error message, then do it with exit()');
            }
        }
        ob_end_clean();
        
        @header("RepoIncPath: " . $incPath, false);
    }
    
    // Autodetect and add TARGET.page.js file
    $jsPath = str_replace('.page.php', '.page.js', $filePath);
    if ( file_exists($jsPath) ) {
        $publicPath = str_replace(realpath(DRUPAL_ROOT), '', realpath($jsPath));
        $publicPath = "/" . ltrim(str_replace("\\", "/", $publicPath), "/");
        // Filenames that has a dot as the first character may not be accessible througn Apache, use this workaround
        if ( substr(basename($jsPath), 0, 1) === '.' ) {
            $publicPath = '/repo_pages_module/js-css-read?path=' . ltrim($publicPath, '/');
        }
        drupal_add_js(
            $publicPath, 
            array(
                'type' => 'file',
                'group' => JS_THEME,
            )
        );
        @header("RepoJsPath: " . $publicPath, false);
    }
    
    // Add targeted JavaScript files registered by the .info file
    foreach($pageInfo['js'] as $index => $jsFile){
        if ( file_exists($jsFile) ) {
            $publicPath = str_replace(realpath(DRUPAL_ROOT), '', realpath($jsPath));
            $publicPath = "/" . ltrim(str_replace("\\", "/", $publicPath), "/");
            // Filenames that has a dot as the first character may not be accessible througn Apache, use this workaround
            if ( substr(basename($jsPath), 0, 1) === '.' ) {
                $publicPath = '/repo_pages_module/js-css-read?path=' . ltrim($publicPath, '/');
            }
            drupal_add_js(
                $publicPath,
                array(
                    'type' => 'file',
                    'group' => JS_THEME,
                )
            );
            @header("RepoJsFromInfoPath: " . $publicPath, false);
        }
    }

    // Autodetect and add TARGET.page.js file
    $cssPath = str_replace('.page.php', '.page.css', $filePath);
    if ( file_exists($cssPath) ) {
        $publicPath = str_replace(realpath(DRUPAL_ROOT), '', realpath($cssPath));
        $publicPath = "/" . ltrim(str_replace("\\", "/", $publicPath), "/");
        // Filenames that has a dot as the first character may not be accessible througn Apache, use this workaround
        if ( substr(basename($cssPath), 0, 1) === '.' ) {
            $publicPath = '/repo_pages_module/js-css-read?path=' . ltrim($publicPath, '/');
        }
        drupal_add_css(
            $publicPath, 
            array(
                'type' => 'external',
                'group' => CSS_THEME,
            )
        );
        @header("RepoCssPath: " . $publicPath, false);
    }
    
    // Add targeted CSS files registered by the .info file
    foreach($pageInfo['css'] as $index => $cssFile){
        if ( file_exists($cssFile) ) {
            $publicPath = str_replace(realpath(DRUPAL_ROOT), '', realpath($cssFile));
            $publicPath = "/" . ltrim(str_replace("\\", "/", $publicPath), "/");
            // Filenames that has a dot as the first character may not be accessible througn Apache, use this workaround
            if ( substr(basename($jsPath), 0, 1) === '.' ) {
                $publicPath = '/repo_pages_module/js-css-read?path=' . ltrim($publicPath, '/');
            }
            drupal_add_css(
                $publicPath,
                array(
                    'type' => 'file',
                    'group' => CSS_THEME,
                )
            );
            @header("RepoCssFromInfoPath: " . $publicPath, false);
        }
    }
    
    // Autodetect and add TARGET.page.less file
    if ( module_exists('less') ) {
        $lessPath = str_replace('.page.php', '.page.less', $filePath);
        if ( file_exists($lessPath) ) {
            @header("RepoLessPath: " . $lessPath);
            drupal_add_css(
                $lessPath, 
                array(
                    'type' => 'file',
                    'group' => CSS_THEME,
                )
            );
            @header("RepoLessPath: " . $publicPath, false);
        }
    }
    
    if ( variable_get('repo_pages_scan', true) === true && $pageInfo['code scan'] == 'strict' ) {
        repo_pages_codescan_scantpl($pageInfo, $filePath);
    }
    
    ob_start();
    $incReturn = include($filePath);
    $html .= ob_get_contents();
    ob_end_clean();
    
    // Scan the rendered-markup generated for issues (per settings)
    if ( variable_get('repo_pages_scan', true) === true && $pageInfo['code scan'] == 'strict' ) {
        $LibXMLErrors = array();
        if ( repo_pages_isValidHTML($html, $LibXMLErrors) === false ) {
            $ErrMsg = 'There is malformed HTML being generated in this page. Please fix the following detected LibXMLErrors:<ul>';
            $errCounter = 0;
            foreach ( $LibXMLErrors as $LibXMLError ) {
                if ( strpos($LibXMLError->message, "htmlParseEntityRef: expecting ';'") === false && strpos($LibXMLError->message, "htmlParseEntityRef: no name") === false ) {
                    $errCounter++;
                    $thisErrMsg = "LibXML-Error: &quot;<i>{$LibXMLError->message}</i>&quot; ";
                    if ( strpos($LibXMLError->message, 'Unexpected end tag') !== false ) {
                        $thisErrMsg = '<b>' . $thisErrMsg . '<img src="/misc/message-24-warning.png" /></b> (<small>this means there is an unclosed tag somewhere</small>)';
                    }
                    $ErrMsg .= '<li>' . $thisErrMsg . '</li>';
                }
            }
            $ErrMsg .= '</ul>';
            if ( $errCounter > 0 ) {
                repo_pages_codescan_reportissue(25, $filePath, $ErrMsg);
            }
        }
    }
    
    // Allow $_GET to override $pageInfo['render mode']
    if ( !empty($_GET['render_mode']) ) {
        $pageInfo['render mode'] = $_GET['render_mode'];
    }
    
    // "solo" rendering mode - Drupal's rendering engine with no be used. ONLY the HTML/[PHP] in this file will be returned to the client-browser. Note, this requieres the usage of the drupal_output_buffering module.
    if ( $pageInfo['render mode'] == 'solo' && intval($allowSoloMode) === 1 ) {
        @ob_end_clean();
        while (@ob_end_clean());
        print $html;
        exit();
    }
    
    // "body" rendering mode - The HTML/[PHP] in this file will be the ONLY markup between the <body> and </body> tag. ALL other markup within the <body> tag is destroyed. Note that this option preserves the <head> tag.
    if ( $pageInfo['render mode'] == 'body' ) {
        $GLOBALS['overrideBodyMarkup'] = $html;
    }

    if ( $incReturn !== 1 ) {
        return $incReturn;
    } else {
        return $html;
    }
}

function _repo_pages_find_pages_files($dir = 'sites/all/pages/', $prefix = '.page.php') {
    
    //error_log("_repo_pages_find_pages_files() has been called with dir={$dir} prefix={$prefix}");
    //error_log($dir);
    
    global $repoPagesScannedDir;
    if ( !isset($repoPagesScannedDir) ){
        $repoPagesScannedDir = array();
    } else {
        if ( in_array(realpath($dir), $repoPagesScannedDir) ) {
            return array();
        } else {
            $repoPagesScannedDir[] = realpath($dir);
        }
    }
    
    $drupalRoot = realpath('./');
    $dir = realpath($dir) . '/';
    $result = array();
    
    if ( is_file($dir . '.page.php') ) {
        $f = $dir . '.page.php';
        $uri = str_replace(array($drupalRoot . '/', $drupalRoot . "\\"), '', $f);
        $uri = str_replace("\\", '/', $uri);
        $uri = str_replace('.page.php', '', $uri);
        $uri = rtrim($uri, '/');
        $result[$uri] = _repo_pages_get_page_info($f);
    }
   $files = glob($dir . '*', GLOB_MARK);
    
    foreach ( $files as $f ) {
        if ( substr($f, -1) === '/' || substr($f, -1) === "\\" ) {
        
            // Bug killer - restrict to DRUPAL_ROOT and sites/default/files
            $realpathDir = realpath($f);
            $realpathDirOver = realpath('sites/default/files/emover/' . $f);
            $realpathDrupalRoot = realpath(DRUPAL_ROOT . '/sites/all/pages');
            $realpathFilesDir = realpath('sites/default/files/emover');
            if ( 
                strpos($realpathDir, $realpathDrupalRoot) !== 0 
                && strpos($realpathDirOver, $realpathDrupalRoot) !== 0 
                && strpos($realpathDir, $realpathFilesDir) !== 0 
                && strpos($realpathDirOver, $realpathFilesDir) !== 0 
            ) {
                //error_log(__FUNCTION__ . " is exiting, {$dir} (rp={$realpathDir},{$realpathDirOver}) is not inside of {$realpathDrupalRoot} nor {$realpathFilesDir} ");
                //return array();
            } else {
                $result = array_merge($result, _repo_pages_find_pages_files($f, $prefix));
            }
        } else {
            if ( substr($f, -strlen($prefix)) == $prefix ) { // this if block checks and verifies the file-extension
                $f = $dir . basename($f);
                $uri = str_replace(array($drupalRoot . '/', $drupalRoot . "\\"), '', $f);
                $uri = str_replace("\\", '/', $uri);
                $uri = str_replace('.page.php', '', $uri);
                $uri = rtrim($uri, '/');
                $result[$uri] = _repo_pages_get_page_info($f);
            }
        }
    }

    return $result;
}

function _repo_pages_get_page_info($pageFilePath) {

    // Default values
    $defaultTitle = strtok($_SERVER['REQUEST_URI'], '?');
    $defaultTitle = array_pop(explode('/', $defaultTitle));
    $defaultTitle = str_replace('-', ' ', $defaultTitle);
    $defaultTitle = ucwords(strtolower($defaultTitle));
    $info = array(
        'title' => $defaultTitle,
        'path' => $pageFilePath,
        'index in solr' => '',
        'inject dev note' => '1',
        'render mode' => 'normal',
        'code scan' => 'strict'
    );
    $infoFilePath = str_replace('.page.php', '.page.info', $pageFilePath);
    if ( file_exists($infoFilePath) && substr($infoFilePath, -10) === '.page.info' ) {
        @header( "RepoPageInfoFinalPath: " . $infoFilePath );
        $iniData = parse_ini_file($infoFilePath);
        if ( !is_array($iniData) ) {
            error_log("Error in _repo_pages_get_page_info() - {$infoFilePath} does not seem to be a valid ini file");
        } else {
            $info = array_merge($info, $iniData);
        }
    }

    // Enforce strict type-casting
    if ( intval($info['index in solr']) === 1 || strtolower($info['index in solr']) === 'true' ) {
        $info['index in solr'] = 1;
    }
    if ( intval($info['inject dev note']) === 1 || strtolower($info['inject dev note']) === 'true' ) {
        $info['inject dev note'] = 1;
    }
    return $info;

}

function repo_pages_codescan_scaninc($infoData, $filePath) {
    
    if ( variable_get('repo_pages_scan', true) !== true ) {
        return;
    }
    if ( $infoData['code scan'] === 'off' ) {
        return;
    }
    
    $incContents = file_get_contents($filePath);
    $incPhpCode = repo_pages_stripNonPhpCode($incContents);
    
    // page.inc files should not contain code written in the PHP alternative syntax 
    if ( 
        strpos($incPhpCode, 'endforeach') <> 0 
        || strpos($incPhpCode, 'endif') <> 0 
        || strpos($incPhpCode, 'endswitch') <> 0
    ) {
        repo_pages_codescan_reportissue(0, $filePath, '*.page.inc files are not allowed to contain <a href="http://php.net/manual/en/control-structures.alternative-syntax.php">PHP-alternative syntax</a> code.');
    }
    
    // *.page.* files should not contain PHP-shorttags
    if ( substr_count($incContents, '<' . '?') > substr_count($incContents, '<' . '?php') ) {
        repo_pages_codescan_reportissue(100, $filePath, 'No *.page.* files should ever contain PHP-shorttags, ever.');
    }
}

function repo_pages_codescan_scantpl($infoData, $filePath) {
    
    if ( variable_get('repo_pages_scan', true) !== true ) {
        return;
    }
    if ( $infoData['code scan'] === 'off' ) {
        return;
    }
    
    $tplContents = file_get_contents($filePath);
    $tplPhpCode = repo_pages_stripNonPhpCode($tplContents);
    $tplMarkup = repo_pages_stripPhpCode($tplContents);
    $spacelessTplMarkup = str_replace(array(" ", "\n", "\t", "\r", "\f"), "", $tplMarkup);
    $renderMode = ( empty($infoData['render mode']) ? 'normal' : $infoData['render mode'] );
    
    // *.page.* files should not contain PHP-shorttags
    if ( substr_count($tplContents, '<' . '?') > substr_count($tplContents, '<' . '?php') ) {
        repo_pages_codescan_reportissue(100, $filePath, 'No *.page.* files should ever contain PHP-shorttags, ever.');
    }
    
    // page.php files must contain PHP alternative syntax with PHP code
    if ( file_exists(str_replace('.page.php', '.page.inc', $filePath)) ) { // except when there is no accompanying *.page.inc file
        if ( 
            substr_count($tplPhpCode, ' if ') > substr_count($tplPhpCode, 'endif')
            || substr_count($tplPhpCode, ' foreach') > substr_count($tplPhpCode, 'endforeach')
            || substr_count($tplPhpCode, ' switch') > substr_count($tplPhpCode, 'endswitch')
        ) {
            repo_pages_codescan_reportissue(25, $filePath, '*.page.php files that contain PHP code must be written in the <a href="http://php.net/manual/en/control-structures.alternative-syntax.php">PHP-alternative syntax</a>.');
        }
    }
    
    // *.page.php should not have any more than 1 db_query() statement
    if ( strpos($tplPhpCode, 'db_query(') !== false ) {
        if ( file_exists(str_replace('.page.php', '.page.inc', $filePath)) ) {
            repo_pages_codescan_reportissue(25, $filePath, '*.page.php files that have an accompanying ~.page.inc file, should not contain very much PHP logic. The PHP logic should be in the ~.page.inc file. Please relocate all logic and db_query() usage into the .inc file.');
        } else {
            if ( substr_count($tplPhpCode, 'db_query(') > 1 ) {
                repo_pages_codescan_reportissue(25, $filePath, '*.page.php files should never have a lot of PHP-logic within them. If you find yourself writting multiple lines of logic, then place that logic in a *.page.inc file. Please relocate this logic, and its multiple usages of db_query(), to a new ~.page.inc file.');
            }
        }
    }
    
    // *.page.php should not contain any more than 1 script tag
    if ( strpos($tplContents, '<script') !== false && $renderMode !== 'solo' ) {
        if ( file_exists(str_replace('.page.php', '.page.js', $filePath)) ) {
            repo_pages_codescan_reportissue(25, $filePath, '*.page.php files that have an accompanying ~.page.js file, should not contain script-tags. The JavaScript should exist in the ~.page.js file. Please relocate all JavaScript to into the .js file. If you are referencing a JavaScript on an external site, you should be using <a href="https://api.drupal.org/api/drupal/includes%21common.inc/function/drupal_add_js/7">drupal_add_js()</a>. You can also reference JavaScripts from the ~.page.info file.');
        } else {
            if ( substr_count($tplContents, '<script') > 1 ) {
                repo_pages_codescan_reportissue(25, $filePath, '*.page.php files should never have a lot of JavaScript within them. If you find yourself writting multiple lines of JavaScript, then place that code in a *.page.js file. Please relocate this code into a new ~.page.js file. If you are referencing a JavaScript on an external site, you should be using <a href="https://api.drupal.org/api/drupal/includes%21common.inc/function/drupal_add_js/7">drupal_add_js()</a>. You can also reference JavaScripts from the ~.page.info file.');
            }
        }
    }
    
    // *.page.php should not contain multiple <br/> tags (3 or more) in a row
    $makrupTest = $spacelessTplMarkup;
    $makrupTest = strtolower($makrupTest);
    $makrupTest = str_replace('<br/>', '<br>', $makrupTest);
    if ( strpos($makrupTest, '<br><br><br>') !== false ) {
        repo_pages_codescan_reportissue(25, $filePath, 'Excessive use of BR tags was detected. If you are finding yourself writing multiple BR tags in a row, please encapsulate the neighbouring content within a div so a themers can apply spacing where necessary.');
    }
    
    // *.page.php should never contain a style="~" in markup, styles should be applied from the .less/.css file
    if ( strpos($tplMarkup, "style=\"") !== false ) {
        $styleTags = explode("style=\"", $tplMarkup);
        foreach ( $styleTags as $index => $styleTag ) {
            if ( $index !== 0 ) {
                // Look only at the property value; blah in style="blah"
                $styleEndPos = strpos($styleTag, "\"");
                if ( $styleEndPos !== false ) {
                    $styleTag = substr($styleTag, 0, $styleEndPos);
                }
                
                // Don't complain about style usage if the style declaration is empty or hiding the HTML element
                if ( 
                    ( trim($styleTag) === '' )
                    || ( strpos($styleTag, 'display') !== false && strpos($styleTag, 'none') !== false )
                    || ( strpos($styleTag, 'display') !== false && strpos($styleTag, 'block') !== false )
                    || ( strpos($styleTag, 'display') !== false && strpos($styleTag, 'inline') !== false )
                    || ( strpos($styleTag, 'display') !== false && strpos($styleTag, 'initial') !== false )
                    || ( strpos($styleTag, 'visibility') !== false && strpos($styleTag, 'hidden') !== false )
                ) {
                    $bn = basename($filePath);
                    error_log("repo_pages_codescan_scantpl() is ignoring a code-scan issue of style-tag usage within {$bn}");
                } else {
                    repo_pages_codescan_reportissue(10, $filePath, 'Style tag usage detected - please do not use style="~" properties within any HTML tag unless the style-tag is only being used to <i><u>display: none;</u></i> an element. All styles should be applied onto HTML element from the .less/.css file(s).');
                    break;
                }
            }
        }
    }
    
    // *.page.php files should never contain any <style> tags
    if ( strpos($tplMarkup, '<style') !== false ) {
        repo_pages_codescan_reportissue(25, $filePath, 'A &lt;style&gt; tag was detected in this file, this is a problem as &lt;style&gt; tags are not supposed to exist within the &lt;body&gt;. Please relocate any styles here to its own ~.page.less or ~.page.css file. If you are referencing an external style-sheet, use <a href="https://api.drupal.org/api/drupal/includes%21common.inc/function/drupal_add_css/7">drupal_add_css()</a> for that.');
    }
    
}

function repo_pages_codescan_reportissue($severity = 0, $filePath, $msg = 'Error') {

    // Report error to the PHP error log
    $rptErrMsg = "RepoPages Code-Scan - ISSUE FOUND - \"{$msg}\" in {$filePath} ";
    error_log($rptErrMsg);
    
    // Report the same error-message to the RepoPages-CodeScan log
    $csLogPath = 'sites/default/files/repo-pages-codescan.log';
    @mkdir(dirname($csLogPath));
    file_put_contents($csLogPath, date('Y-m-d g:ia') . ' - ' . $rptErrMsg . "\n", FILE_APPEND);
    
    // Show this error on the UI
    $rptDrupalMsg = "<b>RepoPages Code-Scan issue was detected!</b><br/>\n{$msg}<br/>\n<small><b>Problem File:</b> {$filePath}</small>";
    drupal_set_message($rptDrupalMsg, 'error');
    
    // Any errors with a $severity that is 50 or greater should kill the PHP thread
    if ( intval($severity) >= 50 ) {
        if ( variable_get('repo_pages_scan_cankill', true) === true ) {
            @ob_end_clean();
            while(@ob_end_clean());
            print "<h1>PHP THREAD KILLED DUE TO REPOPAGES CODE-SCAN ERROR</h1><br/>\n";
            print $rptDrupalMsg;
            exit();
        }
    }
    
}

function repo_pages_stripNonPhpCode($str) {
    
    $ret = array();
    
    $phpOpenTag = '<' . '?';
    $phpCloseTag = '?' . '>';
    $phpTags = array($phpOpenTag, $phpCloseTag);
    
    $splitToken = '-SPLIT-TOKEN-' . time();
    $str = str_replace($phpTags, $splitToken, $str);
    
    $parts = explode($splitToken, $str);
    foreach ( $parts as $part ) {
        if ( substr($part, 0, 3) === 'php' ) {
            $ret[] = $part;
        }
    }
    
    return implode('', $ret);
}

function repo_pages_stripPhpCode($str) {
    $ret = array();
    
    $phpOpenTag = '<' . '?';
    $phpCloseTag = '?' . '>';
    $phpTags = array($phpOpenTag, $phpCloseTag);
    
    $splitToken = '-SPLIT-TOKEN-' . time();
    $str = str_replace($phpTags, $splitToken, $str);
    
    $parts = explode($splitToken, $str);
    foreach ( $parts as $part ) {
        if ( substr($part, 0, 3) !== 'php' ) {
            $ret[] = $part;
        }
    }
    
    return implode('', $ret);
}

/** bool repo_pages_isValidHTML(string $html, [ByReference array &$LibXMLErrors = array()])
 *
 *  Detects if the given HTML is valid (without any structural errors)
 *  
 *  Malformed HTML may still be loaded with PHP's DOMDocument class, all this 
 *  function checks is if there are ANY errors or malformed HTML in the given string.
 *
 *  When this function returns false, $LibXMLErrors will contains all HTML-errors 
 *  related to the input.
 */ 
function repo_pages_isValidHTML($givenHTML, &$LibXMLErrors = array()) {
    
    // Return buffer this function will return
    $ret = null;
    
    // Detect libxml errors within this function, libxml-errors may get triggered from calling DOMDocument as described in http://us1.php.net/manual/en/class.domdocument.php
    $prevLibXmlUseInternalErrorsFlag = libxml_use_internal_errors(true);
    libxml_clear_errors();
    
    // Try loading the given HTML with DOMDocument - http://us1.php.net/manual/en/class.domdocument.php
    $doc = new DOMDocument();
    @$doc->loadHTML($givenHTML);
    
    // Check if there were any libxml errors triggered with the last call of DOMDocument::loadHTML()
    if ( count(libxml_get_errors()) === 0 ) {
        $ret = true;
        $LibXMLErrors = array();
    } else {
        $ret = false;
        $LibXMLErrors = libxml_get_errors();
    }
    
    // Restore libxml error flag to its previous state
    libxml_use_internal_errors($prevLibXmlUseInternalErrorsFlag);
    libxml_clear_errors();
    
    // Return 
    return $ret;
}

function repo_pages_javascript_serv_workaround() {
    
    if ( empty($_GET['path']) ) {
        return 'Missing path parameter';
    }
    
    if ( substr(basename($_GET['path']), 0, 1) !== '.' ) {
        header('HTTP/1.0 403 Forbidden');
        return 'Rejected - This filename does not begin with a dot';
        
    }
    
    if ( substr(basename($_GET['path']), -3) !== '.js'  && substr(basename($_GET['path']), -4) !== '.css' ) {
        header('HTTP/1.0 403 Forbidden');
        return 'Rejected - This filename does not end with .js nor .css';
    }
    
    if ( !file_exists($_GET['path']) ) {
        header("HTTP/1.0 404 Not Found");
        return 'JavaScript File Not Found';
    }
    
    $realFilePath = realpath($_GET['path']);
    $realDrupalRoot = realpath(DRUPAL_ROOT);
    if ( substr($realFilePath, 0, strlen($realDrupalRoot)) !== $realDrupalRoot ) {
        header('HTTP/1.0 403 Forbidden');
        return 'Rejected - The target file is not within the WebRoot';
    }
    
    @ob_end_clean();
    while (@ob_end_clean());
    
    if ( substr(basename($_GET['path']), -3) === '.js' ) {
        header('Content-Type: application/javascript');
    }
    
    if ( substr(basename($_GET['path']), -4) === '.css' ) {
        header('Content-Type: text/css');
    }
    
    readfile($_GET['path']);
    exit();
}









